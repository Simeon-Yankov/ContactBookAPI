{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=ContactBookAPIDb;Username=postgres;Password=postgres"
  },
  "Serilog": {
    "Using": [ "Serilog.Expressions" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/ContactBookLogs-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
